{
  "name": "anthogirard/ps-module-utils",
  "description": "Library to make the module configuration quick & easy to develop. Also provides some useful tools.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Anthony Girard",
      "homepage": "https://github.com/antho-girard"
    }
  ],
  "autoload": {
    "psr-4": {
      "AG\\PSModuleUtils\\": "lib"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AG\\PSModuleUtils\\": "tests/Unit/"
    }
  },
  "require": {
    "php": "~7.2 || ~8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^2.9",
    "phpdocumentor/reflection-docblock": "^5.3",
    "prestashop/module-lib-service-container": "^2.0",
    "symfony/console": "^4.4",
    "symfony/filesystem": "^4.4",
    "symfony/options-resolver": "^4.4",
    "symfony/property-access": "^4.4",
    "symfony/property-info": "^4.4",
    "symfony/serializer": "^4.4",
    "symfony/validator": "^4.4",
    "symfony/yaml": "^4.4",
    "alcohol/iso4217": "^3.1",
    "prestashop/decimal": "^1.5",
    "moneyphp/money": "^3.3",
    "ircmaxell/random-lib": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "prestashop/php-dev-tools": "^3.16"
  }
}
