{
  "name": "anthogirard/ps-module-utils",
  "description": "Library to make the module configuration quick & easy to develop. Also provides some useful tools.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Anthony Girard",
      "homepage": "https://github.com/antho-girard"
    }
  ],
  "autoload": {
    "psr-4": {
      "AG\\PSModuleUtils\\": "lib"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AG\\PSModuleUtils\\": "tests/Unit/"
    }
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^1.27",
    "phpdocumentor/reflection-docblock": "^3.3",
    "prestashop/module-lib-service-container": "^1.4",
    "symfony/console": "^3.4",
    "symfony/filesystem": "^3.4",
    "symfony/options-resolver": "^3.4",
    "symfony/property-access": "^3.4",
    "symfony/property-info": "^3.4",
    "symfony/serializer": "^3.4",
    "symfony/validator": "^3.4",
    "symfony/yaml": "^3.4",
    "alcohol/iso4217": "^3.1",
    "prestashop/decimal": "^1.4",
    "moneyphp/money": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "prestashop/php-dev-tools": "^v3.4"
  }
}
